{
  "name": "@agentgate/client",
  "version": "0.2.17",
  "description": "TypeScript client for AgentGate API",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "eventsource": "^2.0.2"
  },
  "devDependencies": {
    "@types/eventsource": "^1.1.15",
    "typescript": "^5.7.2",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {},
  "keywords": [
    "agentgate",
    "ai",
    "agent",
    "automation",
    "api",
    "client"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentgate/agentgate.git",
    "directory": "packages/client"
  }
}
